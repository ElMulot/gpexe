{% extends 'modal/std_modal.html.twig' %}
{% form_theme form 'form/layout.html.twig' %}

{% block modal_body %}

	{{ encore_entry_link_tags('form') }}
	
	{{ form(form) }}

{% endblock %}

{% block modal_footer %}
	
	<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{% trans %}Cancel{% endtrans %}</button>
	<button type="submit" form="form" class="btn btn-primary">{% trans %}Save{% endtrans %}</button>

{% endblock %}